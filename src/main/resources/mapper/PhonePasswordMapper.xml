<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.example.mybank.mapper.PhonePasswordMapper">

    <resultMap id="BaseResultMap" type="org.example.mybank.entity.PhonePassword">
            <id property="passwordId" column="passwordId" jdbcType="INTEGER"/>
            <result property="phoneNumber" column="phoneNumber" jdbcType="CHAR"/>
            <result property="password" column="password" jdbcType="VARCHAR"/>
    </resultMap>

    <sql id="Base_Column_List">
        passwordId,phoneNumber,password
    </sql>
</mapper>
