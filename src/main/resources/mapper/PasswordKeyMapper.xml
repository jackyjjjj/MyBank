<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.example.mybank.mapper.PasswordKeyMapper">

    <resultMap id="BaseResultMap" type="org.example.mybank.entity.PasswordKey">
            <id property="id" column="id" jdbcType="INTEGER"/>
            <result property="accountNumber" column="accountNumber" jdbcType="CHAR"/>
            <result property="passwordKey" column="passwordKey" jdbcType="VARCHAR"/>
    </resultMap>

    <sql id="Base_Column_List">
        id,accountNumber,passwordKey
    </sql>
</mapper>
